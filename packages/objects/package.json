{
  "name": "@hypernetlabs/objects",
  "version": "0.0.1",
  "description": "Objects and types shared by the Hypernet Protocol",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoHypernet/hypernet-protocol.git"
  },
  "bugs": {
    "url": "https://github.com/GoHypernet/hypernet-protocol/issues"
  },
  "homepage": "https://github.com/GoHypernet/hypernet-protocol/tree/master/packages/objects#readme",
  "author": "Charlie Sibbach <charlie@hyperdyne.io>",
  "keywords": [
    "Blockchain",
    "Hypernet",
    "SDK",
    "Ethereum",
    "Vector"
  ],
  "main": "dist/src/index.js",
  "files": [
    "dist",
    "src",
    "errors",
    "types",
    "!dist/test",
    "!dist/tsconfig.tsbuildinfo"
  ],
  "scripts": {
    "build": "yarn clean && yarn compile",
    "clean": "rimraf dist",
    "compile": "tsc && yarn copy-files && yarn update-package-json && yarn format:pkg && npx tsc-alias -p tsconfig.json",
    "copy-files": "copyfiles -u 1 src/**/*.d.ts dist/src/",
    "format": "prettier --write \"src/**/*.ts\" && yarn format:pkg",
    "format:pkg": "format-package -w",
    "generate-docs": "npx typedoc --options typedoc.json",
    "lint": "tslint -p src/tsconfig.json",
    "prepare": "yarn build",
    "test": "yarn compile && jest --runInBand --coverage --passWithNoTests",
    "update-package-json": "ts-node ../../scripts/importDevDependencies.ts",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "types": "dist/src/index.d.ts",
  "dependencies": {
    "class-transformer": "^0.3.2",
    "ethers": "5.0.31",
