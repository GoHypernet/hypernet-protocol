    "randomstring": "^1.1.5",
    "react-test-renderer": "16.13.1",
    "rimraf": "^3.0.2",
    "sass-loader": "8.0.0",
    "source-map-loader": "^2.0.0",
    "stream-http": "^3.1.1",
    "style-loader": "^2.0.0",
    "testdouble": "^3.16.1",
    "testdouble-jest": "^2.0.0",
    "ts-jest": "^26.3.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "tsc-alias": "^1.2.6",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typedoc": "^0.20.16",
    "typescript": "^4.2.4",
    "url-loader": "3.0.0",
    "wait-on": "^5.2.0",
    "webpack": "^5.19.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}
