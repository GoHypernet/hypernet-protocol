{
  "name": "@hypernetlabs/hypernet-contracts",
  "version": "0.0.1",
  "description": "Solidity smart contracts for the Hypernet Protocol",
  "keywords": [
    "Solidity",
    "Hypernet",
    "Blockchain",
    "Ethereum",
    "Embark",
    "Ganache",
    "Truffle",
    "Nitro",
    "State",
    "Channels"
  ],
  "author": "Charlie Sibbach <charlie@hyperdyne.io>",
  "homepage": "https://github.com/GoHypernet/hypernet-protocol/tree/master/packages/contracts#readme",
  "license": "ISC",
  "main": "lib/contracts.js",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "devDependencies": {
    "@types/express-serve-static-core": "4.0.53",
    "embark": "^6.0.0",
    "embark-basic-pipeline": "^6.0.0",
    "embark-geth": "^6.0.0",
    "embark-graph": "^6.0.0",
    "embark-ipfs": "^6.0.0",
    "embark-parity": "^6.0.0",
    "embark-profiler": "^6.0.0",
    "embark-swarm": "^6.0.0",
    "embark-whisper-geth": "^6.0.0",
    "embarkjs": "^6.0.0",
    "embarkjs-ens": "^6.0.0",
    "embarkjs-ipfs": "^6.0.0",
    "embarkjs-swarm": "^6.0.0",
    "embarkjs-web3": "^6.0.0",
    "embarkjs-whisper": "^6.0.0",
    "scrypt": "https://github.com/barrysteyn/node-scrypt/"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^3.1.0",
    "@openzeppelin/contracts-ethereum-package": "^3.0.0",
    "@statechannels/channel-client": "^0.2.0",
    "@statechannels/nitro-protocol": "^0.1.6",
    "concurrently": "^5.3.0",
    "embark-ethereum-blockchain-client": "^6.0.0",
    "embark-ganache": "^6.0.0",
    "embark-scripts-runner": "^6.0.0",
    "env-cmd": "^10.1.0",
    "ethers": "^5.0.8",
    "wait-on": "^5.2.0"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoHypernet/hypernet-protocol.git"
  },
  "scripts": {
    "start-build": "yarn install-state-channels && yarn build-state-channels && yarn start-with-envs",
    "start-run": "yarn start-with-envs",
    "install-state-channels": "yarn --cwd ../../../subtrees/statechannels/ install",
    "build-state-channels": "yarn --cwd ../../../subtrees/statechannels/ build",
    "start-with-envs": "env-cmd -f ./.env.development --use-shell \"yarn start-wallet-and-ganache\"",
    "start-shared-ganache": "yarn --cwd ../../../subtrees/statechannels/packages/devtools/ start:shared-ganache",
    "start-web-wallet": "yarn --cwd ../../../subtrees/statechannels/packages/xstate-wallet/ start",
    "start-wallet-and-ganache": "concurrently \"yarn start-shared-ganache\" \"wait-on tcp:8545 && yarn start-web-wallet\""
  },
  "bugs": {
    "url": "https://github.com/GoHypernet/hypernet-protocol/issues"
  }
}
